package Login;

public class LogVo {
	
	private String id;
	private String pwd;

	public LogVo() {

	}

	public LogVo(String id, String pwd) {
		this.id = id;
		this.pwd = pwd;
	
	}
	
	public void userPrint() {
		System.out.printf("%10s %10s\n ", id,pwd);
	}
	
	public static void userTitlePrint() {
		System.out.println("======================================");
		System.out.printf("%10s %10s\n ", "회원아이디","회원비밀번호");
		System.out.println("======================================");
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getPwd() {
		return pwd;
	}

	public void setPwd(String pwd) {
		this.pwd = pwd;
	}

	public void Print() {
		System.out.printf("%10s %10s %10s ", id, pwd);
	}

}
