import java.util.LinkedList;

public class Ex100 {
	
	
	
	public Ex100() {
		start();
		
	}
	
	public void start() {
		//LinkedList => list,deque,queqe
		//queue일때는 입력순서 유지출력은 먼저 입력된객체가 출력된다.
		//출력된객체는 컬렉션에서 지워진다.
		//Deque일때는 양쪽에서 객체를 입력또는 출력할수있다
		LinkedList<String> ll = new LinkedList<String>();
		
		//객체추가
		//offer()데이터넣기
		ll.offer("홍길동");
		ll.offer(new String("세종대왕"));
		ll.offer(String.valueOf(1234));
		ll.offer(5678+"");
		
		System.out.println("size->"+ll.size());
		
		//객체 얻어오기
		//pop() -데이터출력
		
		String data = ll.pop();
		System.out.println("data="+data);
		System.out.println("size---->"+ll.size());
		
	}
	
	
	
	public static void main(String[] args) {
		new Ex100();
		
		}
		
		

	}

}
