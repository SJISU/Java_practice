import java.util.HashMap;

public class HashMapTest {

	public HashMapTest() {
		//Map interface를 상속받은 클래스는 key,value 가 존재한다
		//1.HashMapdm운 key와 value데이터를 저장한다
		//2.TreeMap은 key와 value로 데이터를 저장하고,key기준으로 정렬된다
		
		HashMap<Integer, String > hm = new HashMap<Integer, String >();
		//객체명.put(k,y) -> 객체추가
		hm.put(1, "홍길동");
		hm.put(100, "세종대왕");
		hm.put(50, "이순신");     //HashMap저장소 객체안에 k와v를 각각 순서대로 정렬해서 저장,,1하면 홍길동이나오고 100하면 세종대왕이나오고...
		
		//객체명.get(k,y)-> 값얻어오는것,값가져오는것
		String name = hm.get(100); //100을 썻지만 int가아니고 intdger로 보는것->자바가상머신이형변환해주고잇음
		System.out.println("100->"+name);
		
		HashMap<String, MemberVO> hm2 = new HashMap<String, MemberVO>();
		
		
		
	}

	
	
	
	
	
	public static void main(String[] args) {
		new HashMapTest();

	}

}
