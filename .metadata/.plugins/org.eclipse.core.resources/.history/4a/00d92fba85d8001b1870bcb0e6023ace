import java.awt.Frame;

// 컨테이너 Frame상속받아 구현하기
public class FrameTest extends Frame    {

	public FrameTest() {
		//창의 제목
		setTitle("프레임생성연습중");
		//창의 크기(pixel) ->상속받은 Frame 의 상위클래스 Window클래스 기능사용 -> setBounds(),setSize()...
		setSize(500,400); //상위클래스의 메소드를 내거처럼 사용
		//창을 보여주기-숨기는기능도있음 ->크기설정한  창을 보여주는...-> setVisible()		
		setVisible(true); //true:보여줌 false:안보여줌(삭제x)  //여기까지 하고 컴파일하면 아무것도없는 창이뜬다
		
		
		

	}

	public static void main(String[] args) {
		new FrameTest();
		
	}

}
